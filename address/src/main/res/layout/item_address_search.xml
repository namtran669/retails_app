<?xml version="1.0" encoding="utf-8"?><androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="wrap_content"    android:background="@drawable/bg_border_catskillwhite_bottom_1_width"    android:foreground="?android:selectableItemBackground"    android:orientation="vertical">    <ImageView        android:id="@+id/iconAddressImageView"        android:layout_width="22dp"        android:layout_height="22dp"        android:layout_marginStart="20dp"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent"        app:srcCompat="@drawable/ic_others_location_blue"        tools:ignore="ContentDescription" />    <TextView        android:id="@+id/addressTitleTextView"        style="@style/H3.Trout"        android:layout_width="0dp"        android:layout_height="wrap_content"        android:layout_marginStart="24dp"        android:layout_marginTop="20dp"        android:layout_marginEnd="5dp"        android:ellipsize="end"        android:maxLines="1"        android:text="@string/home"        app:layout_constraintBottom_toTopOf="@+id/fullAddressTextView"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintHorizontal_bias="0.5"        app:layout_constraintStart_toEndOf="@+id/iconAddressImageView"        app:layout_constraintTop_toTopOf="parent"        app:layout_constraintVertical_chainStyle="packed" />    <TextView        android:id="@+id/fullAddressTextView"        style="@style/P2.Trout50"        android:layout_width="0dp"        android:layout_height="wrap_content"        android:layout_marginStart="24dp"        android:layout_marginEnd="5dp"        android:layout_marginBottom="20dp"        android:ellipsize="end"        android:maxLines="1"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintHorizontal_bias="0.5"        app:layout_constraintStart_toEndOf="@+id/iconAddressImageView"        app:layout_constraintTop_toBottomOf="@+id/addressTitleTextView"        tools:text="Sukhumvit Road, Bang Chak, Phra Khaâ€¦" /></androidx.constraintlayout.widget.ConstraintLayout>