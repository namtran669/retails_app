<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="AppTheme" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="AppTheme.FullScreen">
        <item name="android:windowIsTranslucent">true</item>
    </style>

    <style name="AppTheme.FullScreen.WhiteStatusBar">
        <item name="android:windowLightStatusBar" tools:targetApi="23">true</item>
        <item name="android:statusBarColor">@android:color/white</item>
    </style>

    <style name="AppTheme.FullScreen.BlackStatusBar">
        <item name="android:statusBarColor">@android:color/black</item>
    </style>

    <style name="AppToolbar">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">?attr/actionBarSize</item>
        <item name="contentInsetStart">0dp</item>
    </style>

    <style name="SearchAppToolbar">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">?attr/actionBarSize</item>
        <item name="contentInsetStart">0dp</item>
    </style>

    <style name="AppToolbarDefault" parent="AppToolbar">
        <item name="popupTheme">@style/ThemeOverlay.AppCompat.Light</item>
        <item name="android:background">@android:color/white</item>
    </style>

    <style name="AppToolbarBlack" parent="AppToolbar">
        <item name="popupTheme">@style/ThemeOverlay.AppCompat.Light</item>
        <item name="android:background">@android:color/black</item>
    </style>

    <!--Custom dialog animation-->
    <style name="TranslateUpDownAnimation">
        <item name="android:windowEnterAnimation">@anim/translate_up</item>
        <item name="android:windowExitAnimation">@anim/translate_down</item>
