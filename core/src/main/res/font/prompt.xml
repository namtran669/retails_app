<?xml version="1.0" encoding="utf-8"?>
<font-family tools:ignore="UnusedAttribute"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <font
        android:font="@font/circular_book"
        android:fontStyle="normal"
        android:fontWeight="400"

        app:font="@font/circular_book"
        app:fontStyle="normal"
        app:fontWeight="400" />

    <font
        android:font="@font/circular_medium"
        android:fontStyle="normal"
        android:fontWeight="500"

        app:font="@font/circular_medium"
        app:fontStyle="normal"
        app:fontWeight="500"/>

    <font
        android:font="@font/circular_bold"
        android:fontStyle="normal"
        android:fontWeight="700"

        app:font="@font/circular_bold"
        app:fontStyle="normal"
        app:fontWeight="700"/>

</font-family>