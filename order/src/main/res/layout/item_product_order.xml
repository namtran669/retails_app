<?xml version="1.0" encoding="utf-8"?><androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:orientation="vertical"    android:layout_width="match_parent"    android:layout_height="wrap_content"    android:background="@android:color/white">        <TextView        android:id="@+id/orderNoTextView"        android:layout_width="38dp"        android:layout_height="wrap_content"        android:layout_marginTop="10dp"        android:gravity="center"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent"        tools:text="1"        style="@style/R15.Trout" />    <TextView        android:id="@+id/orderNameTextView"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginTop="10dp"        android:gravity="end"        android:layout_marginStart="2dp"        app:layout_constraintStart_toEndOf="@id/orderNoTextView"        app:layout_constraintTop_toTopOf="parent"        tools:text="Pad Thai With Shrimps"        style="@style/R15.Trout" /></androidx.constraintlayout.widget.ConstraintLayout>